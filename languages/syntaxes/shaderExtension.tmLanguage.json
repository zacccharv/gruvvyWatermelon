{
	"$schema": "https://raw.githubusercontent.com/RedCMD/TmLanguage-Syntax-Highlighter/main/vscode.tmLanguage.schema.json",
	"name": "Unity Shader Extension",
	"scopeName": "source.shader.unity.extension",
	"injectionSelector": "L:source.shader.unity",
	"patterns": [
		{ "include": "#line_comments" },
		{ "include": "#block_comments" },
		{ "include": "#shader-block" },
		{ "include": "#fallback-value" },
		{ "include": "#keywords" }
	],
	"repository": {
		"shader-block": {
			"name": "shader.block.unity",
			"begin": "(Shader)(\\s)(\\\")(.*)(\\\")((\\{)|())",
			"beginCaptures": {
				"1": { "name": "support.class.unity" },
				"3": { "name": "string.quoted.double.begin.whitespace.unity" },
				"4": { "name": "string.quoted.double.whitespace.unity shader.path.unity" },
				"5": { "name": "string.quoted.double.end.whitespace.unity" },
				"6": { "name": "punctuation.brackets.curly.unity.hlsl" }
			},
			"end": "(.\\z)",
			"patterns": [
				{ "include": "#line_comments" },
				{ "include": "#block_comments" },
				{ "include": "#properties-block" },
				{ "include": "#subshader-block" },
				{ "include": "#fallback-value" }
			]
		},
		"subshader-block": {
			"name": "subshader.block.unity",
			"begin": "(SubShader)((\\{)|())",
			"beginCaptures": {
				"1": { "name": "support.class.unity" },
				"2": { "name": "punctuation.brackets.curly.unity.hlsl" }
			},
			"end": "(SubShader|Fallback|Properties)\\s*",
			"endCaptures": { "1": { "name": "support.class.unity" } },
			"patterns": [
				{ "include": "#unity-tag-block" },
				{
					"match": "(LOD)\\s+(\\d+)",
					"captures": {
						"1": { "name": "support.class.unity" },
						"2": { "name": "constant.numeric.unity.hlsl" }
					}
				},
				{ "include": "#keywords" },
				{ "include": "#unity-pass-block" },
				{ "include": "#unity-keywords" },
				{ "include": "#preprocessor" },
				{ "include": "#whitespace-strings" },
				{ "include": "#line_comments" },
				{ "include": "#block_comments" },
				{ "include": "#punctuation" }
			]
		},
		"unity-pass-block": {
			"name": "pass.block.unity",
			"begin": "(Pass)((\\{)|())",
			"beginCaptures": {
				"1": { "name": "support.class.unity" },
				"2": { "name": "punctuation.brackets.curly.unity.hlsl" }
			},
			"end": "(Tags|SubShader|Fallback)\\s+(?=(\\{|\\\"))",
			"endCaptures": { "1": { "name": "support.class.unity" } },
			"patterns": [
				{ "include": "#keywords" },
				{ "include": "#unity-keywords" },
				{ "include": "#qualifiers" },
				{ "include": "#attributes" },
				{ "include": "#functions" },
				{ "include": "#function_calls" },
				{ "include": "#member_access" },
				{ "include": "#constants" },
				{ "include": "#variables" },
				{ "include": "#types" },
				{ "include": "#preprocessor" },
				{ "include": "#whitespace-strings" },
				{ "include": "#punctuation" }
			]
		},
		"properties-block": {
			"name": "properties.block.unity",
			"begin": "(Properties)((\\{)|())",
			"beginCaptures": {
				"1": { "name": "support.class.unity" },
				"2": { "name": "punctuation.brackets.curly.unity.hlsl" }
			},
			"end": "(\\s\\})",
			"patterns": [
				{
					"name": "keyword.attribute",
					"match": "\\[(HideInInspector|Normal|HDR|ToggleUI|NoScaleOffset|ToggleOff|MainColor|MainTexture)\\]"
				},
				{ "include": "#keywords" },
				{ "include": "#constants" },
				{ "include": "#functions" },
				{ "include": "#function_calls" },
				{ "include": "#punctuation" },
				{ "include": "#whitespace-strings" }
			]
		},
		"fallback-value": {
			"patterns": [
				{
					"match": "(Fallback)(\\\")(.*)(\\\")",
					"captures": {
						"1": { "name": "support.class.unity" },
						"2": { "name": "string.quoted.double.begin.whitespace.unity" },
						"3": { "name": "string.quoted.double.whitespace.unity shader.path.unity" },
						"4": { "name": "string.quoted.double.end.whitespace.unity" }
					}
				},
				{
					"match": "(Fallback)\\s+(Off)",
					"captures": {
						"1": { "name": "support.class.unity" },
						"2": { "name": "support.class.param.unity" }
					}
				}
			]
		},
		"unity-keywords": {
			"patterns": [
				{
					"name": "support.class.unity",
					"match": "(?<!\\\")\\b(VSTUMARKER|AlphaToMask|LOD|Shader|Properties|SubShader|Pass|UsePass|Texture|Fog|Tags|BindChannels|GrabPass|Category|Color|Range|Float|Vector|2D|2DArray|Rect|Cube|Constant|Previous|TexGen|LightmapMode|SetTexture|Lighting|Material|Cull|ZTest|ZWrite|ZClip|Fog|AlphaMask|AlphaTest|Blend|BlendOp|Offset|SeparateSpecular|ColorMaterial|Diffuse|Ambient|Specular|Shininess|Emission|Combine|ConstantColor|Matrix|Lerp|Mode|Density|LightMode|RequireOptions|LightTexCount|Name|Bind|UsePass|TextureScale|TextureSize|BorderScale|Fall[Bb]ack|Stencil|Ref)\\b"
				},
				{
					"name": "support.class.unity",
					"match": "(?<!\\\")\\b(Comp|Fail|ZFail|CompBack|PassBack|FailBack|ZFailBack|CompFront|CustomEditor|PassFront|FailFront|ZFailFront)\\b"
				},
				{
					"match": "(?<!\\\")\\b(ColorMask)\\s(([RGBA][RGBA]?[RGBA]?[RGBA]?)|(0))\\b",
					"captures": {
						"1": { "name": "support.class.unity" },
						"3": { "name": "literal.rgba" },
						"4": { "name": "constant.numeric.decimal.unity.hlsl" }
					}
				},
				{
					"match": "(?<!\\\")\\b(Ref|WriteMask|ReadMask)\\s+([0-9]*)\\b",
					"captures": {
						"1": { "name": "support.class.unity" },
						"3": { "name": "constant.numeric.decimal.unity.hlsl" }
					}
				},
				{
					"name": "support.class.param.unity",
					"match": "\\b(On|Off|ObjectLinear|EyeLinear|SphereMap|CubeReflect|CubeNormal|ARGB|AmbientAndDiffuse|Emission|Back|Front|Linear|Exp|Exp2|One|Zero|AppSrcAdd|AppDstAdd|Vertex|VertexOrNone|VertexOnly|Pixel|PixelOnly|VertexOrPixel|VertexAndPixel|PixelOrNone|None|SoftVegetation|Normal|Tangent|Mesh|Normals|Tangents|World|Texcoord|Texcoord0|Texcoord1|Disabled)\\b"
				},
				{
					"name": "support.class.param.unity",
					"match": "\\b(SrcColor|SrcAlpha|DstColor|DstAlpha|OneMinusSrcColor|OneMinusSrcAlpha|OneMinusDstColor|OneMinusDstAlpha)\\b"
				},
				{
					"name": "support.class.param.unity",
					"match": "\\b(ADD|SUB|REVSUB|MIN|MAX|LOGICALCLEAR|LOGICALSET|LOGICALCOPY|LOGICALCOPYINVERTED|LOGICALNOOP|LOGICALINVERT|LOGICALAND|LOGICALNAND|LOGICALOR|LOGICALNOR|LOGICALXOR|LOGICALEQUIV|LOGICALANDREVERSE|LOGICALANDINVERTED|LOGICALORREVERSE|LOGICALORINVERTED|MULTIPLY|SCREEN|OVERLAY|DARKEN|LIGHTEN|COLORDODGE|COLORBURN|HARDLIGHT|SOFTLIGHT|DIFFERENCE|EXCLUSION|HSLHUE|HSLSATURATION|HSLCOLOR|HSLLUMINOSITY)\\b"
				},
				{
					"name": "support.class.param.unity",
					"match": "\\b(Never|Less|Equal|LEqual|Greater|NotEqual|GEqual|Always)\\b"
				},
				{
					"name": "support.class.param.unity",
					"match": "\\b(Keep|Replace|IncrSat|DecrSat|Invert|IncrWrap|DecrWrap)\\b"
				},
				{
					"match": "\\b(CGPROGRAM|GLSLPROGRAM|HLSLPROGRAM|ENDCG|ENDGLSL|ENDHLSL|C\\+\\+RAW|BEGIN DOC|END DOC|BEGIN EX|END EX|CSRAW|CSNONE|CLASS|CUSTOM|CUSTOM_PROP|THREAD_SAFE|BEGIN|END|TYPEMAP|ENUM|DOCUMENT|AUTO|AUTO_PROP|AUTO_PTR_PROP|OBSOLETE|THREAD_UNSAFE|CONVERTEXAMPLE|NONSEALED_CLASS|CONDITIONAL|STRUCT)\\b",
					"name": "keyword.preprocessor.region.unity"
				},
				{
					"match": "\\b(Dependency|Fallback)\\b",
					"name": "support.variable.unity"
				},
				{
					"match": "\\b(_\\w+)\\b",
					"name": "support.variable.input.unity"
				},
				{
					"match": "\\b(uv(2)*_\\w+|viewDir|COLOR|screenPos|worldPos|worldRefl|worldNormal|worldRefl|worldNormal)\\b",
					"name": "support.variable.input.unity"
				},
				{
					"match": "\\b(Albedo|Normal|Emission|Specular|Gloss|Alpha)\\b",
					"name": "support.variable.output.unity"
				},
				{
					"match": "\\b(appdata_(base|tan|full|img)|SurfaceOutput)\\b",
					"name": "support.variable.structure.unity"
				},
				{
					"match": "\\b(UNITY_MATRIX_(MVP|MV|V|P|VP|T_MV|IT_MV|TEXTURE0|TEXTURE1|TEXTURE2|TEXTURE3)|_Object2World|_World2Object|_WorldSpaceCameraPos|unity_Scale)\\b",
					"name": "support.variable.transformation.unity"
				},
				{
					"match": "\\b(_ModelLightColor[0-9]|_SpecularLightColor[0-9]|_ObjectSpaceLightPos|_Light2World|_World2Light|_Object2Light)\\b",
					"name": "support.variable.lighting.unity"
				},
				{
					"match": "\\b(_Time|_SinTime|_CosTime|unity_DeltaTime|_ProjectionParams|_ScreenParams)\\b",
					"name": "support.variable.other.unity"
				}
			]
		},
		"unity-tag-block": {
			"begin": "(^\\s*\\b(Tags)\\b\\s*(\\{)?)",
			"beginCaptures": {
				"1": { "name": "support.class.unity" },
				"3": { "name": "punctuation.brackets.curly.unity.hlsl" }
			},
			"end": "(\\}+)",
			"endCaptures": { "0": { "name": "punctuation.brackets.curly.unity.hlsl" } },
			"patterns": [
				{
					"comment": "Built-in unity tag words",
					"match": "(\\\")(RenderType|Queue|IgnoreProjector|ForceNoShadowCasting|PreviewType|DisableBatching|LightMode|RequireOptions|ForceVertex|CanUseSpriteAtlas|SRPBatcherAlwaysCompatible)(\\\")",
					"captures": {
						"1": { "name": "string.quoted.double.begin" },
						"2": { "name": "string.unity.tagblock.key" },
						"3": { "name": "string.quoted.double.end" }
					}
				},
				{
					"comment": "tag values",
					"match": "(\\\")(TransparentCutout|Transparent|Background|Geometry|AlphaTest|AlphaTestShadowCaster)(\\+)([0-9]*)(\\\")",
					"captures": {
						"1": { "name": "string.quoted.double.begin" },
						"2": { "name": "string.unity.tagblock.value" },
						"3": { "name": "keyword.operator" },
						"4": { "name": "constant.numeric" },
						"5": { "name": "string.quoted.double.end" }
					}
				},
				{
					"comment": "tag values",
					"match": "(\\\")(Opaque|TransparentCutout|Transparent|Background|Geometry|AlphaTest|AlphaTestShadowCaster|ForwardBase|ForwardAdd|Deferred|Meta|ShadowCaster|Always|Auto|True|False|Yes|No)(\\\")",
					"captures": {
						"1": { "name": "string.quoted.double.begin" },
						"2": { "name": "string.unity.tagblock.value" },
						"3": { "name": "string.quoted.double.end" }
					}
				},
				{ "include": "#keywords" },
				{ "include": "#constants" },
				{ "include": "#whitespace-strings" }
			]
		},
		"line_comments": {
			"comment": "single line comment",
			"name": "comment.line.double-slash.unity.hlsl",
			"match": "\\s*//.*"
		},
		"block_comments": {
			"patterns": [
				{
					"comment": "empty block comments",
					"name": "comment.block.unity.hlsl",
					"match": "/\\*\\*/"
				},
				{
					"comment": "block documentation comments",
					"name": "comment.block.documentation.unity.hlsl",
					"begin": "/\\*\\*",
					"end": "\\*/",
					"patterns": [ { "include": "#block_comments" } ]
				},
				{
					"comment": "block comments",
					"name": "comment.block.unity.hlsl",
					"begin": "/\\*(?!\\*)",
					"end": "\\*/",
					"patterns": [ { "include": "#block_comments" } ]
				}
			]
		},
		"attributes": {
			"comment": "attribute declaration",
			"name": "constant.language.attribute.unity.hlsl",
			"match": "\\[(?i:domain|earlydepthstencil|instance|maxtessfactor|numthreads|outputcontrolpoints|outputtopology|partitioning|patchconstantfunc|flatten|branch|loop|unroll|fastopt|allow_uav_condition|forcecase|call|clipplanes|maxvertexcount|noinline|unknown|rootsignature|raypayload|shader)(\\(.*\\))?\\]"
		},
		"preprocessor": {
			"patterns": [
				{
					"comment": "preprocessor",
					"name": "keyword.preprocessor.unity.hlsl",
					"match": "^\\s*#\\s*(if|elif|else|endif|ifdef|ifndef|undef|include|include_with_pragmas|line|error|warning|pragma|INF)\\s+(\\\"[^\\\"]*\\\"|[\\w\\(\\)]+)(.*)$",
					"captures": {
						"1": { "name": "keyword.other.preprocessor.unity.hlsl" },
						"2": {
							"name": "keyword.preprocessor.unity.hlsl",
							"patterns": [
								{
									"comment": "defined macro",
									"match": "(defined)\\(\\w+\\)",
									"captures": { "1": { "name": "entity.name.function" } }
								},
								{ "include": "#whitespace-strings" }
							]
						},
						"3": {
							"name": "variable.other.unity.hlsl",
							"patterns": [
								{ "include": "#block_comments" },
								{ "include": "#line_comments" }
							]
						}
					}
				},
				{
					"comment": "define preprocessor",
					"name": "keyword.preprocessor.unity.hlsl",
					"match": "^\\s*#\\s*(define)\\s+(\\w+)(\\s*\\(\\s*[\\w\\,\\s]+\\s*\\))?\\s+(.*)?",
					"captures": {
						"1": { "name": "keyword.preprocessor.unity.hlsl" },
						"2": { "name": "entity.name.function.preprocessor.unity.hlsl" },
						"3": { "name": "variable.other.unity.hlsl" },
						"4": {
							"name": "variable.other.unity.hlsl",
							"patterns": [
								{ "include": "#line_comments" },
								{ "include": "#block_comments" },
								{ "include": "#keywords" },
								{ "include": "#attributes" },
								{ "include": "#function_calls" },
								{ "include": "#constants" },
								{ "include": "#types" },
								{ "include": "#variables" },
								{ "include": "#punctuation" }
							]
						}
					}
				}
			]
		},
		"functions": {
			"comment": "function definition",
			"begin": "\\b(\\w+)(\\s*\\<\\s*(\\w+)\\s*\\>\\s*)?\\s+(\\w+)?([\\s]*\\()",
			"beginCaptures": {
				"1": {
					"name": "entity.name.type.unity.hlsl",
					"patterns": [ { "include": "#types" } ]
				},
				"3": {
					"name": "entity.name.type.unity.hlsl",
					"patterns": [ { "include": "#types" } ]
				},
				"4": { "name": "entity.name.function.unity.hlsl" },
				"6": { "name": "punctuation.brackets.round.unity.hlsl" }
			},
			"end": "\\{",
			"endCaptures": { "0": { "name": "punctuation.brackets.curly.unity.hlsl" } },
			"patterns": [
				{ "include": "#member_access" },
				{ "include": "#preprocessor" },
				{ "include": "#line_comments" },
				{ "include": "#block_comments" },
				{ "include": "#qualifiers" },
				{ "include": "#keywords" },
				{ "include": "#attributes" },
				{ "include": "#function_calls" },
				{ "include": "#constants" },
				{ "include": "#types" },
				{ "include": "#variables" },
				{ "include": "#punctuation" },
				{ "include": "#whitespace-strings" }
			]
		},
		"function_calls": {
			"comment": "function/method calls",
			"begin": "(?:\\s+|\\[)(?!if|else|while|for|switch|do|return)\\b([a-zA-Z_][a-zA-Z0-9_]*)(\\s*\\<\\s*(\\w+)\\s*\\>)?([\\s]*\\()",
			"beginCaptures": {
				"1": {
					"name": "entity.name.function.unity.hlsl",
					"patterns": [ { "include": "#types" } ]
				},
				"4": { "name": "punctuation.brackets.round.unity.hlsl" }
			},
			"end": "\\)",
			"endCaptures": { "0": { "name": "punctuation.brackets.round.unity.hlsl" } },
			"patterns": [
				{ "include": "#member_access" },
				{ "include": "#qualifiers" },
				{ "include": "#unity-keywords" },
				{ "include": "#keywords" },
				{ "include": "#function_calls" },
				{ "include": "#constants" },
				{ "include": "#types" },
				{ "include": "#variables" },
				{ "include": "#punctuation" },
				{ "include": "#whitespace-strings" }
			]
		},
		"constants": {
			"patterns": [
				{
					"comment": "decimal float literal",
					"match": "(-?\\b[0-9][0-9]*(\\.)[0-9]*)([eE][+-]?[0-9]+)?((?i:[fhl]))?\\b",
					"captures": {
						"1": { "name": "constant.numeric.float.unity.hlsl" },
						"2": { "name": "punctuation.decimal.point" },
						"3": { "name": "constant.language.attribute.hlsl entity.name.tag" },
						"4": { "name": "constant.language.attribute.hlsl entity.name.tag" }
					}
				},
				{
					"comment": "decimal literal",
					"match": "(-?\\b((0x)[0-9a-fA-F]+|(0)[0-9]+|[0-9]+))([ul]+)?\\b",
					"captures": {
						"2": { "name": "constant.numeric.decimal.unity.hlsl" },
						"3": { "name": "constant.language.attribute.hlsl entity.name.tag" },
						"4": { "name": "constant.language.attribute.hlsl entity.name.tag" },
						"5": { "name": "constant.language.attribute.hlsl entity.name.tag" }
					}
				},
				{
					"comment": "boolean constant",
					"name": "constant.language.boolean.unity.hlsl",
					"match": "\\b(true|false)\\b"
				},
				{
					"name": "constant.language.boolean.unity.hlsl",
					"match": "\\b(FALSE|TRUE|NULL)\\b"
				},
				{
					"comment": "hlsl semantic",
					"name": "constant.language.unity.hlsl",
					"match": "(?<=\\:\\s|\\:)(?i:BINORMAL[0-9]*|BLENDINDICES[0-9]*|BLENDWEIGHT[0-9]*|COLOR[0-9]*|NORMAL[0-9]*|POSITIONT|POSITION[0-9]*|PSIZE[0-9]*|TANGENT[0-9]*|TEXCOORD[0-9]*|FOG|TESSFACTOR[0-9]*|VFACE|VPOS|DEPTH[0-9]*)\\b"
				},
				{
					"comment": "hlsl semantic shader model 4.0",
					"name": "constant.language.sm4.unity.hlsl",
					"match": "\\b(?<=\\:\\s|\\:)(?i:SV_ClipDistance[0-9]*|SV_CullDistance[0-9]*|SV_Coverage|SV_Depth|SV_DepthGreaterEqual[0-9]*|SV_DepthLessEqual[0-9]*|SV_InstanceID|SV_IsFrontFace|SV_Position|SV_RenderTargetArrayIndex|SV_SampleIndex|SV_StencilRef|SV_Target[0-7]?|SV_VertexID|SV_ViewportArrayIndex)\\b"
				},
				{
					"comment": "hlsl semantic shader model 5.0",
					"name": "constant.language.sm5.unity.hlsl",
					"match": "(?<=\\:\\s|\\:)(?i:SV_DispatchThreadID|SV_DomainLocation|SV_GroupID|SV_GroupIndex|SV_GroupThreadID|SV_GSInstanceID|SV_InsideTessFactor|SV_OutputControlPointID|SV_TessFactor)\\b"
				},
				{
					"comment": "hlsl semantic shader model 5.1",
					"name": "constant.language.sm51.unity.hlsl",
					"match": "(?<=\\:\\s|\\:)(?i:SV_InnerCoverage|SV_StencilRef)\\b"
				}
			]
		},
		"types": {
			"comment": "types",
			"patterns": [
				{
					"comment": "scalar Types",
					"name": "storage.type.unity.hlsl",
					"match": "\\b(bool|int|uint|dword|half|float|double|fixed)\\b"
				},
				{
					"comment": "minimum precision scalar Types",
					"name": "storage.type.unity.hlsl",
					"match": "\\b(min16float|min10float|min16int|min12int|min16uint)\\b"
				},
				{
					"comment": "scalar Types 6.0",
					"name": "storage.type.sm60.unity.hlsl",
					"match": "\\b(uint64_t|int64_t|uint32_t|int32_t)\\b"
				},
				{
					"comment": "scalar Types 6.2",
					"name": "storage.type.sm62.unity.hlsl",
					"match": "\\b(float16_t|uint16_t|int16_t|float32_t|float64_t)\\b"
				},
				{
					"comment": "vector/matrix types",
					"name": "storage.type.unity.hlsl",
					"match": "\\b(matrix|vector|(bool|int|uint|half|float|double|dword|fixed)[1-4](x[1-4])?)\\b"
				},
				{
					"comment": "vector/matrix types",
					"name": "storage.type.unity.hlsl",
					"match": "\\b((min12int|min16float|min16int|min16uint|float16_t|float32_t|float64_t|uint16_t|int16_t|uint32_t|int32_t|uint64_t|int64_t)[1-4](x[1-4])?)\\b"
				},
				{
					"comment": "string type",
					"name": "storage.type.unity.hlsl",
					"match": "\\b(string)\\b"
				},
				{
					"comment": "read hlsl type",
					"name": "storage.type.object.unity.hlsl",
					"match": "\\b(AppendStructuredBuffer|Buffer|StructuredBuffer|ByteAddressBuffer|ConstantBuffer|TextureBuffer|ConsumeStructuredBuffer|InputPatch|OutputPatch|FeedbackTexture2D|FeedbackTexture2DArray)\\b"
				},
				{
					"comment": "rasterized order view type",
					"name": "storage.type.object.rasterizerordered.unity.hlsl",
					"match": "\\b(RasterizerOrderedBuffer|RasterizerOrderedByteAddressBuffer|RasterizerOrderedStructuredBuffer|RasterizerOrderedTexture1D|RasterizerOrderedTexture1DArray|RasterizerOrderedTexture2D|RasterizerOrderedTexture2DArray|RasterizerOrderedTexture3D)\\b"
				},
				{
