				},
				{
					"comment": "legacy sampler type",
					"name": "storage.type.sampler.legacy.unity.hlsl",
					"match": "\\b(sampler|sampler1D|sampler2D|sampler3D|samplerCUBE|sampler_state)\\b"
				},
				{
					"comment": "sampler type",
					"name": "storage.type.sampler.unity.hlsl",
