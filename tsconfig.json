{
    "extends": "@tsconfig/node22/tsconfig.json",
    "compilerOptions": {
        "module": "ES2022",
        "moduleResolution": "bundler",
        "resolveJsonModule": true,
        "skipLibCheck": true,
        "strict": true,
        "strictNullChecks": true,
        "paths": {
            "@/*": [
                "./src/*"
            ]
        }
    }
}