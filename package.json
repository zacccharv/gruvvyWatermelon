{
  "name": "gruvvy-watermelon",
  "displayName": "Gruvvy Watermelon",
  "description": "Personal version of gruvbox",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Themes"
  ],
  "contributes": {
    "themes": [
      {
        "label": "Gruvvy Watermelon",
        "uiTheme": "vs-dark",
        "path": "./themes/Gruvvy-Watermelon-color-theme.json"
      }
    ]
  },
  "scripts": {
    "start": "nodemon --watch src src/index.js",
    "build": "node src/index.js && mkdir -p build",
    "prepublishOnly": "npm run build && vsce publish"
  },
  "dependencies": {
    "nodemon": "^3.0.3"
  }
}